@import '../../../node_modules/materialize-css/bin/materialize.css'
@import 'nib'

color1 = #125688
color_text1 = #212121
GrisGoogle = #f1f1f1
VerdeSubHeader = #00796b
ColorCardUser = #f9f9f9

body
	// background-color color1
	// font-family 'Montserrat'
	width 100%
	height 100vh
	font-size 13px

.App_Container__box
	height 100%
	display flex
	flex-direction column
	overflow hidden
	.main-container
		flex 1 auto
		display flex
		flex-direction column
		&__box
			flex 1 100%
			display flex
			flex-direction column

.landing
	margin 0 auto
	display flex
	align-items center
	height 100vh
	.iphone
		max-width 100%
	.content-box
		display flex
		align-items center

.not-margin
	margin 0

.line-divider
	margin 10px 0 20px
	border-bottom 1px solid #d4d4d4

.avatar-socio
	margin 0 auto
	background-size cover
	width 40px
	height 40px
	background-repeat no-repeat
	background-positon center
	border-radius 50%

.fixed-action-btn
	right 100px
	bottom 40px
	a
		baclground-color rgba(0,188,212,.87)

	
@import './signin/style.styl'
@import './header/style.styl'
@import './content/style.styl'
@import './info_user/style.styl'
@import './subheader/style.styl'
